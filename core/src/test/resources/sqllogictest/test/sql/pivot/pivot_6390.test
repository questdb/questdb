# can use with inside the subquery
query IIIII rowsort pivot
WITH CPB AS (
SELECT 'C1' AS CPDH, 2022 AS NF, 10 AS JG
UNION ALL
SELECT 'C1',2018,20
UNION ALL
SELECT 'C1',2017,0
UNION ALL
SELECT 'C2',2022,10
UNION ALL
SELECT 'C2',2010,30
UNION ALL
SELECT 'C3',2010,80
)
SELECT * FROM CPB PIVOT (sum(jg) FOR nf IN (SELECT DISTINCT NF FROM CPB) GROUP BY CPDH) ORDER BY CPDH;

require parquet

query I
select created, approx_percentile(latency, 0.9, 3) latency
from parquet_scan('data/sql-testing/demo_telemetry_wal.parquet') timestamp(created)
where
  event = 105 -- event is fixed
  and tableid = 10
  and rowCount > 0
sample by 1s
fill(0)
limit 10

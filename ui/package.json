    "dotenv": "^10.0.0",
    "echarts": "4.8.0",
    "glyphicons-halflings": "1.9.1",
    "jquery": "3.5.1",
    "monaco-editor": "^0.30.1",
    "ramda": "0.27.1",
    "react": "16.13.1",
    "react-contextmenu": "2.14.0",
    "react-dom": "16.13.1",
    "react-popper": "2.2.3",
    "react-redux": "7.2.0",
    "react-transition-group": "4.4.1",
    "react-virtuoso": "^2.2.6",
    "redux": "4.0.5",
    "redux-observable": "1.2.0",
    "resize-observer-polyfill": "1.5.1",

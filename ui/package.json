    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin": "6.1.0",
    "handlebars": "4.7.7",
    "handlebars-loader": "1.7.1",
    "html-webpack-plugin": "4.3.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sass": "1.26.8",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "stylelint": "^13.13.1",

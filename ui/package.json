    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "author": {
    "name": "QuestDB"
  },
  "lint-staged": {
    "**/**/*.{js.jsx,js,es6,jsx,ts,tsx,md,yml}": [
      "prettier --config ./prettier.config.js --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}

    "@questdb/sql-grammar": "1.0.9",
    "@types/ramda": "0.27.40",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-transition-group": "4.4.0",
    "@types/styled-components": "5.1.7",
    "@types/throttle-debounce": "2.1.0",
    "ace-builds": "1.4.11",
    "animate.css": "3.7.2",
